	<queryString>
		<![CDATA[SELECT
     pedidos.`id` AS pedidos_id,
     pedidos.`fechapedido` AS pedidos_fechapedido,
     pedidos.`nropedido` AS pedidos_nropedido,
     pedidos.`empleadoid` AS pedidos_empleadoid,
     detalle_pedidos.`productosid` AS detalle_pedidos_productosid,
     detalle_pedidos.`cantidad` AS detalle_pedidos_cantidad,
     empleados.`Nombre` AS empleados_Nombre,
     empleados.`CI` AS empleados_CI,
     productos.`Descripcion` AS productos_Descripcion,
     productos.`Stock` AS productos_Stock
FROM
     `pedidos` pedidos INNER JOIN `detalle_pedidos` detalle_pedidos ON pedidos.`id` = detalle_pedidos.`pedidosid`
     INNER JOIN `empleados` empleados ON pedidos.`empleadoid` = empleados.`id`
     INNER JOIN `productos` productos ON detalle_pedidos.`productosid` = productos.`id`


WHERE pedidos.nropedido= $P!{p_nropedido}]]>
	</queryString>
